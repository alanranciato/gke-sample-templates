apiVersion: networking.istio.io/v1alpha3
kind: VirtualService
metadata:
  name: hello-svc
spec:
  http:
  - match:
    - uri:
        prefix: /v3
    route:
    - destination:
        host: hello3
        subset: v3
  - match:
    - uri:
        prefix: /v4
    route:
    - destination:
        host: hello4
        subset: v4